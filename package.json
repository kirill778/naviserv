{
  "name": "csv-data-processor",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:start": "docker-compose -f docker-compose.db.yml up -d",
    "db:stop": "docker-compose -f docker-compose.db.yml down",
    "server:dev": "nodemon --watch server --ext ts,json --exec \"node --loader ts-node/esm server/server.ts\"",
    "server:build": "tsc --project tsconfig.server.json",
    "server:start": "node dist/server/server.js",
    "python:install": "pip install -r requirements.txt",
    "python:dev": "uvicorn app:app --reload --host 0.0.0.0 --port 3001",
    "python:start": "uvicorn app:app --host 0.0.0.0 --port 3001",
    "python:docker": "docker-compose -f docker-compose.python.yml up -d",
    "python:docker:stop": "docker-compose -f docker-compose.python.yml down"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "html-to-image": "^1.11.11",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.344.0",
    "mathjs": "^12.4.0",
    "multer": "^1.4.5-lts.2",
    "papaparse": "^5.4.1",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.4.4",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.10.4",
    "sequelize": "^6.37.7",
    "streamifier": "^0.1.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/papaparse": "^5.3.14",
    "@types/pg": "^8.15.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-grid-layout": "^1.3.5",
    "@types/sequelize": "^4.28.20",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
